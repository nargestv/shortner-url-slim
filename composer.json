{
    "require": {
        "guzzlehttp/psr7": "^1",
        "php-di/php-di": "^6.0",
        "sapphirecat/slim4-http-interop-adapter": "^1.0",
        "selective/basepath": "^2.1",
        "slim/psr7": "^1.5",
        "slim/slim": "4.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
